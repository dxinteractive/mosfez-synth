{
  "name": "mosfez-synth",
  "version": "0.0.4",
  "description": "A microtonal-aware synth engine library for web.",
  "keywords": [
    "microtonal",
    "xen",
    "synth"
  ],
  "homepage": "https://github.com/dxinteractive/mosfez-synth",
  "repository": {
    "type": "git",
    "url": "git@github.com:dxinteractive/mosfez-synth.git"
  },
  "bugs": {
    "url": "https://github.com/dxinteractive/mosfez-synth/issues"
  },
  "author": "Damien Clarke <dxinteractive@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prep": "yarn && yarn clean && cd demo && yarn",
    "build": "rm -rf dist && yarn rollup -c",
    "test": "yarn jest",
    "lint": "yarn eslint src",
    "pretty": "yarn prettier --write .",
    "prepare": "yarn build && cp dist/* .",
    "postpublish": "yarn clean",
    "clean": "rm -f touch-start.* synth.* faust.* types.* poly.*"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "esbuild": "^0.14.42",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^28.1.0",
    "prettier": "2.6.2",
    "rollup": "^2.75.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "sideEffects": false,
  "files": [
    "touch-start.*",
    "synth.*",
    "faust.*",
    "types.*",
    "poly.*",
    "public"
  ],
  "dependencies": {
    "mosfez-faust": "^0.0.11"
  }
}
